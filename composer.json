{
    "name": "techguy/world-countries-api",
    "description": "This package has api pointing to https://restcountries.eu. Fetches and stores all the countries in your local database.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0",
        "webpatser/laravel-uuid": "^3.0|^4.0"
    },
    "keywords": [
        "laravel",
        "world",
        "countries"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "dev-techguy",
            "email": "info@shiftech.co.ke"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "World\\Countries\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "World\\Countries\\WorldCountriesServiceProvider"
            ]
        }
    }
}
